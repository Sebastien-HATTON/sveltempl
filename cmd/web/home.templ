package web

templ Home() {
	@Base() {
		<h1>Home</h1>
		<div>
			<a href="/page">Page</a>
		</div>
		<div class="boxes">
			<div class="box" id="hash-from-go-vite-aware-1" data-props={ homeProps() }>
				@Svelte("BoxOne", []byte(homeProps()))
			</div>
			<div class="box" id="hash-from-go-vite-aware-2">
				@Svelte("BoxTwo", nil)
			</div>
		</div>
	}
}

func homeProps() string {
	return `{"name": "isomorphic props!"}`
}
